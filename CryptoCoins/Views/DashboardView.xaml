             xmlns:local="clr-namespace:CryptoCoins"
             x:Class="CryptoCoins.Views.DashboardView">
    <BlazorWebView x:Name="blazorWebView" HostPage="wwwroot/index.html">
        <BlazorWebView.RootComponents>
            <RootComponent x:Name="rootComponent" Selector="#app" ComponentType="{x:Type local:Routes}"/>
        </BlazorWebView.RootComponents>
    </BlazorWebView>
</ContentView>
